/******************** Add Table: "ODMXCV"."CV_ActionType" ************************/

/* Build Table Structure */
CREATE TABLE "ODMXCV"."CV_ActionType"
(
	"Term" VARCHAR NOT NULL,
	"Name" VARCHAR NOT NULL,
	"Definition" VARCHAR NULL,
	"Category" VARCHAR NULL,
	"Provenance" VARCHAR NULL,
	"ProvenanceURI" VARCHAR NULL,
	"Note" VARCHAR NULL,
	"ProducesResult" BOOL NULL
) WITHOUT OIDS;

/* Add Primary Key */
ALTER TABLE "ODMXCV"."CV_ActionType" ADD CONSTRAINT "pkCV_ActionType"
	PRIMARY KEY ("Term");

/* Add Comments */
COMMENT ON COLUMN "ODMXCV"."CV_ActionType"."Term" IS 'Controlled vocabulary term';

COMMENT ON COLUMN "ODMXCV"."CV_ActionType"."Name" IS 'The human readable version of the vocabulary term. May be the same as the term.';

COMMENT ON COLUMN "ODMXCV"."CV_ActionType"."Definition" IS 'Definition of vocabulary term.';

COMMENT ON COLUMN "ODMXCV"."CV_ActionType"."Category" IS 'A category within which the term falls.';

COMMENT ON COLUMN "ODMXCV"."CV_ActionType"."Provenance" IS 'The provenance of the vocabulary';

COMMENT ON COLUMN "ODMXCV"."CV_ActionType"."ProvenanceURI" IS 'A link to the source vocabulary from which the term was derived.';

COMMENT ON COLUMN "ODMXCV"."CV_ActionType"."Note" IS 'A note about the vocabulary';

COMMENT ON TABLE "ODMXCV"."CV_ActionType" IS 'Table containing terms used in the ActionType controlled vocabulary.';


