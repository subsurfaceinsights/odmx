	"FeatureGeometryWKT" VARCHAR(8000) NULL,
	"Elevation_m" FLOAT NULL,
	"ElevationDatumCV" VARCHAR NULL,
	"Latitude" FLOAT NULL,
	"Longitude" FLOAT NULL,
	"EPSG" VARCHAR(20) NULL
) WITHOUT OIDS;

/* Add Primary Key */
