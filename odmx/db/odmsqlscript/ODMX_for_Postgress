/******************** Add Table: "ODMXCore"."Results" ************************/

/* Build Table Structure */
CREATE TABLE "ODMXCore"."Results"
(
	"ResultID" BIGINT DEFAULT nextval('"ODMXCore"."Results_ResultID_seq"'::regclass) NOT NULL,
	"ResultUUID" VARCHAR(36) NOT NULL,
	"FeatureActionID" INTEGER NOT NULL,
	"ResultTypeCV" VARCHAR NOT NULL,
	"VariableID" INTEGER NOT NULL,
	"UnitsID" INTEGER NOT NULL,
	"ProcessingLevelID" INTEGER NOT NULL,
	"ResultDateTime" TIMESTAMP NULL,
	"ResultDateTimeUTCOffset" BIGINT NULL,
	"ValidDateTime" TIMESTAMP NULL,
	"ValidDateTimeUTCOffset" BIGINT NULL,
	"StatusCV" VARCHAR NULL,
	"ValueCount" INTEGER NOT NULL,
	"NoDataValue" DOUBLE PRECISION NULL
) WITHOUT OIDS;

/* Add Primary Key */
ALTER TABLE "ODMXCore"."Results" ADD CONSTRAINT "pkResults"
	PRIMARY KEY ("ResultID");

/* Add Comments */
COMMENT ON COLUMN "ODMXCore"."Results"."ResultID" IS 'Primary key identifier.';

COMMENT ON COLUMN "ODMXCore"."Results"."ResultUUID" IS 'A universally unique identifier for the Result';

COMMENT ON COLUMN "ODMXCore"."Results"."FeatureActionID" IS 'Foreign key identifier for the combinaton of SamplingFeature and Action that created the Result';

COMMENT ON COLUMN "ODMXCore"."Results"."ResultTypeCV" IS 'CV term describing the result type (e.g., time series, measurement, etc.)';

COMMENT ON COLUMN "ODMXCore"."Results"."VariableID" IS 'Foreign key identifier for the Variable of the result';

COMMENT ON COLUMN "ODMXCore"."Results"."UnitsID" IS 'Foreign key identifier for the Units of the result';

COMMENT ON COLUMN "ODMXCore"."Results"."ProcessingLevelID" IS 'Foreign key identifier for the ProcessingLevel of the result';

COMMENT ON COLUMN "ODMXCore"."Results"."ResultDateTime" IS 'Date and time at which the result became available (could be used as analysis time for samples)';

COMMENT ON COLUMN "ODMXCore"."Results"."ResultDateTimeUTCOffset" IS 'UTDCOffset of ResultDateTime';

COMMENT ON COLUMN "ODMXCore"."Results"."ValidDateTime" IS 'Date and time for which the result is valid (e.g., for a forecast result).  Should probably be expressed as a duration';

COMMENT ON COLUMN "ODMXCore"."Results"."ValidDateTimeUTCOffset" IS 'UTCOffset of ValidDateTime';

COMMENT ON COLUMN "ODMXCore"."Results"."StatusCV" IS 'Status of the result (e.g., finished, ongoing, etc.)';

COMMENT ON COLUMN "ODMXCore"."Results"."ValueCount" IS 'Number of data values contained within the result. For instance we could have a timeseries data with 1000s of results or a chemical analysis with a sd';

COMMENT ON COLUMN "ODMXCore"."Results"."NoDataValue" IS 'A numeric value indicating no data.';

COMMENT ON TABLE "ODMXCore"."Results" IS 'Describes the results of observation actions (e.g., groups of one or more numeric data values that result from an observation action)';

/* Add Indexes */
CREATE UNIQUE INDEX "Results_ResultUUID_Idx" ON "ODMXCore"."Results" ("ResultUUID");


