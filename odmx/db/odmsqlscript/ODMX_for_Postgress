/************ Update: Schemas ***************/

/* Add Schema: ODMXAnnotations */
CREATE SCHEMA "ODMXAnnotations";
COMMENT ON SCHEMA "ODMXAnnotations" IS 'Annotations are used to add comments or qualifiers to entity instances in ODMX.';

/* Add Schema: ODMXCV */
CREATE SCHEMA "ODMXCV";
COMMENT ON SCHEMA "ODMXCV" IS 'Controlled vocabularies for ODMX.';

/* Add Schema: ODMXCore */
CREATE SCHEMA "ODMXCore";
COMMENT ON SCHEMA "ODMXCore" IS 'The core of ODMX.  Every ODMX instance will implement the Core.';

/* Add Schema: ODMXDataQuality */
CREATE SCHEMA "ODMXDataQuality";
COMMENT ON SCHEMA "ODMXDataQuality" IS 'Entities and attributes related to data quality.';

/* Add Schema: ODMXEquipment */
CREATE SCHEMA "ODMXEquipment";
COMMENT ON SCHEMA "ODMXEquipment" IS 'Entities and attributes of equipment used to make observations (e.g., sensors and instruments).';

/* Add Schema: ODMXExtensionProperties */
CREATE SCHEMA "ODMXExtensionProperties";
COMMENT ON SCHEMA "ODMXExtensionProperties" IS 'A general extensibility mechanism for any entity/object/table in ODMX.';

/* Add Schema: ODMXExternalIdentifiers */
CREATE SCHEMA "ODMXExternalIdentifiers";
COMMENT ON SCHEMA "ODMXExternalIdentifiers" IS 'Entities and attributes for linking ODMX entities (e.g., Sites, Methods, etc.) to external identifier systems.';

/* Add Schema: ODMXLabAnalyses */
CREATE SCHEMA "ODMXLabAnalyses";
COMMENT ON SCHEMA "ODMXLabAnalyses" IS 'Information about samples and laboratory analyses for ex-situ data.';

/* Add Schema: ODMXMicrobial */
CREATE SCHEMA "ODMXMicrobial";

/* Add Schema: ODMXProvenance */
CREATE SCHEMA "ODMXProvenance";
COMMENT ON SCHEMA "ODMXProvenance" IS 'Entities and attributes for creating and storing provenance information and versions of observations in ODMX.';

/* Add Schema: ODMXResults */
CREATE SCHEMA "ODMXResults";
COMMENT ON SCHEMA "ODMXResults" IS 'The resulting values of observations in ODMX.';

/* Add Schema: ODMXSSI */
CREATE SCHEMA "ODMXSSI";
COMMENT ON SCHEMA "ODMXSSI" IS 'This has the SSI extension for Phenocam data';

/* Add Schema: ODMXSSIERT */
CREATE SCHEMA "ODMXSSIERT";
COMMENT ON SCHEMA "ODMXSSIERT" IS 'Schemas to do with ERT data';

/* Add Schema: ODMXSamplingFeatures */
CREATE SCHEMA "ODMXSamplingFeatures";
COMMENT ON SCHEMA "ODMXSamplingFeatures" IS 'Information about sampling features, including sites and specimens.';

/* Add Schema: ODMXSimulation */
CREATE SCHEMA "ODMXSimulation";


